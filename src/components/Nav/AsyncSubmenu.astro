---
import { getStoryblokLinks } from "@utils";
import NavSubmenu from "./NavSubmenu.astro";

const { parentUrl, parentTitle, linkPrefix } = Astro.props;

const links = await getStoryblokLinks(linkPrefix);
---

<NavSubmenu
	parentUrl={parentUrl}
	parentTitle={parentTitle}
	children={links.map((link) => ({
		title: link.name,
		url: `/${link.slug}`,
	}))}
/>
